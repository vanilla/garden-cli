{
	"name": "vanilla/garden-cli",
	"description": "A full-featured, yet ridiculously simple commandline parser for your next php cli script. Stop fighting with getopt().",
	"license": "MIT",
	"authors": [
		{
			"name": "Todd Burry",
			"email": "todd@vanillaforums.com"
		}
	],
	"require": {
		"php": ">=7.0"
	},
	"config": {
		"platform": {
			"php": "7.0.26"
		}
	},
	"autoload": {
		"psr-4": {
			"Garden\\Cli\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Garden\\Cli\\Tests\\": "tests"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^6",
		"digitickets/phpunit-errorhandler": "^4.0"
	}
}
